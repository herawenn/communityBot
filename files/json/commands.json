{
  "shodanhost": {
    "description": "Get detailed information about a specific host from Shodan.",
    "usage": "..shodanhost <IP address>",
    "example": "..shodanhost 8.8.8.8"
  },
  "shodansearch": {
    "description": "Search Shodan for specific terms.",
    "usage": "..shodansearch <search query>",
    "example": "..shodansearch 'Apache server'"
  },
  "shodanscan": {
    "description": "Scan a specific host using Shodan. Note: Requires Shodan scan credits.",
    "usage": "..shodanscan <IP address>",
    "example": "..shodanscan 192.168.1.1"
  },
  "shodanscanstatus": {
    "description": "Check the status of a Shodan scan.",
    "usage": "..shodanscanstatus <scan_id>",
    "example": "..shodanscanstatus 7891234567890"
  },
  "shodanscanresults": {
    "description": "Get the results of a Shodan scan.",
    "usage": "..shodanscanresults <scan_id>",
    "example": "..shodanscanresults 7891234567890"
  },
  "shodanhelp": {
    "description": "Displays help for the Shodan commands.",
    "usage": "..shodanhelp",
    "example": "..shodanhelp"
  },
  "recon": {
    "description": "Perform network reconnaissance using Nmap.",
    "usage": "..recon <target> [options]",
    "example": "..recon 192.168.1.10 -sS -T4"
  },
  "reconhelp": {
    "description": "Displays help for the 'recon' command.",
    "usage": "..reconhelp",
    "example": "..reconhelp"
  },
  "cancelscan": {
    "description": "Cancels the ongoing Nmap scan.",
    "usage": "..cancelscan",
    "example": "..cancelscan"
  },
  "aiscan": {
    "description": "Generate a vulnerability assessment for a given IP address.",
    "usage": "..aiscan <ip_address>",
    "example": "..aiscan 192.168.1.1"
  },
  "categories": {
    "description": "Lists all exploit categories.",
    "usage": "..categories",
    "example": "..categories"
  },
  "exploits": {
    "description": "Searches for exploits by keyword.",
    "usage": "..exploits <keyword>",
    "example": "..exploits wordpress"
  },
  "download": {
    "description": "Downloads a specific exploit by ID.",
    "usage": "..download <exploit_id>",
    "example": "..download 44645"
  },
  "kick": {
    "description": "Kicks a user from the server.",
    "usage": "..kick <user> [reason]",
    "example": "..kick @user Spamming"
  },
  "ban": {
    "description": "Bans a user from the server.",
    "usage": "..ban <user> [reason]",
    "example": "..ban @user Being toxic"
  },
  "unban": {
    "description": "Unbans a user from the server.",
    "usage": "..unban <user#1234>",
    "example": "..unban ExampleUser#1234"
  },
  "mute": {
    "description": "Mutes a user in the server.",
    "usage": "..mute <user> [reason] [duration in seconds]",
    "example": "..mute @user Spamming 600"
  },
  "unmute": {
    "description": "Unmutes a user in the server.",
    "usage": "..unmute <user>",
    "example": "..unmute @user"
  },
  "clear": {
    "description": "Deletes a specified number of messages. Requires 'Manage Messages' permissions.",
    "usage": "..clear <amount>",
    "example": "..clear 5"
  },
  "warn": {
    "description": "Warns a user.",
    "usage": "..warn <user> [reason]",
    "example": "..warn @user For spamming"
  },
  "search": {
    "description": "Search Private dbs for OSINT data.",
    "usage": "..search <query_type> <query>",
    "example": "..search email example@example.com"
  },
  "removeaccount": {
    "description": "Helps with removing your account from specific platforms.",
    "usage": "..removeaccount <platform>",
    "example": "..removeaccount facebook"
  },
  "deindex": {
    "description": "Provides guidance on deindexing your information from Google.",
